# Configuration pour l'environnement de production
# Copiez ce fichier en .env.production et remplissez les valeurs

# Configuration de l'application
VITE_APP_ENV=production
VITE_APP_NAME="IA Foundations"
VITE_APP_VERSION=1.0.0
VITE_APP_BASE_URL=/

# Configuration Supabase (remplacer par vos valeurs)
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Configuration API
VITE_API_BASE_URL=/api
VITE_API_TIMEOUT=30000

# Fonctionnalités
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# Configuration du thème
VITE_THEME=light
VITE_PRIMARY_COLOR=#1e40af
VITE_SECONDARY_COLOR=#64748b

# Configuration des logs
VITE_LOG_LEVEL=warn
VITE_LOG_CONSOLE=true
VITE_LOG_REMOTE=false

# Sécurité
VITE_SECURE_COOKIES=true
VITE_CSRF_PROTECTION=true

# Performance
VITE_ENABLE_CACHING=true
VITE_CACHE_TTL=3600
VITE_LAZY_LOAD=true
