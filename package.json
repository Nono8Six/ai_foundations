{
  "name": "ai-foundations-monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "pnpm dev:frontend",
    "dev:frontend": "pnpm --filter frontend dev",
    "start": "pnpm dev:frontend",
    "build": "pnpm --filter frontend build",
    "preview": "pnpm --filter frontend preview",
    "test": "cross-env-shell LOG_LEVEL=fatal vitest run --passWithNoTests",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "supabase": "cross-env-shell npx supabase@%SUPABASE_CLI_VERSION%",
    "supabase:status": "pnpm supabase status",
    "supabase:start": "pnpm supabase start",
    "supabase:stop": "pnpm supabase stop",
    "supabase:db:pull": "pnpm supabase db pull",
    "supabase:db:push": "pnpm supabase db push",
    "supabase:db:reset": "pnpm supabase db reset",
    "supabase:gen:types": "pnpm supabase gen types typescript --schema public > apps/frontend/src/types/database.types.ts",
    "db:pull": "pnpm supabase:db:pull",
    "db:start": "pnpm supabase:start",
    "db:stop": "pnpm supabase:stop",
    "gen:types": "pnpm supabase:gen:types"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.27.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.2.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "pino": "^9.7.0",
    "sonner": "^2.0.5"
  }
}
