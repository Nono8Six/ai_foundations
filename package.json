{
  "name": "ai-foundations-monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "build": "pnpm --filter frontend build",
    "preview": "pnpm --filter frontend preview",
    "start": "pnpm run dev",
    "test": "pnpm --filter frontend test",
    "test:output": "pnpm --filter frontend test:output",
    "test:watch": "pnpm --filter frontend test:watch",
    "test:coverage": "pnpm --filter frontend test:coverage",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky",
    "gen:types": "node --experimental-json-modules scripts/gen-types.js",
    "validate:env": "node scripts/validate-env.js"
  },
  "supabase": {
    "projectRef": "$SUPABASE_PROJECT_REF",
    "apiUrl": "$SUPABASE_URL",
    "apiKey": "$SUPABASE_ANON_KEY"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.31.0",
    "@rollup/rollup-win32-x64-msvc": "^4.45.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^8.0.0",
    "envalid": "^8.1.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "execa": "^8.0.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.6.2",
    "supabase": "^2.31.8",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@remix-run/router": "^1.23.0",
    "@supabase/supabase-js": "^2.52.1",
    "pino": "^9.7.0",
    "sonner": "^2.0.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,mdx}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
